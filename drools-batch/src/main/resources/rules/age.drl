import com.digiup.sandbox.model.Person;
import java.util.Date;

rule "derived age"
no-loop
dialect "mvel"
    when
        $p : Person(age == null, dateOfBirth != null)
    then
        int age = new Date().getYear() - $p.getDateOfBirth().getYear();
        System.out.println(age);
        modify ($p) {
            setAge(age)
        }
end
